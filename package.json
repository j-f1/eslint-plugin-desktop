{
  "name": "eslint-plugin-desktop",
  "version": "0.0.0",
  "description": "Custom rules used in the GitHub Desktop codebase",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Jed Fox",
  "main": "lib/index.js",
  "scripts": {
    "docs": "eslint-docs",
    "docs:check": "eslint-docs check",
    "lint": "eslint 'lib/**/*.js' 'tests/**/*.js' 'docs/**/*.md'",
    "test": "npm -s run lint && npm -s run mocha && npm -s run docs:check",
    "mocha": "mocha tests --recursive"
  },
  "dependencies": {
    "eslint-config-prettier": "^2.3.0",
    "eslint-docs": "0.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-typescript": "github:nzakas/eslint-plugin-typescript#26f2609",
    "estraverse": "^4.2.0",
    "requireindex": "~1.1.0",
    "typescript-eslint-parser": "^7.0.0"
  },
  "devDependencies": {
    "chalk": "^2.1.0",
    "diff": "^3.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.1.2",
    "ora": "^1.3.0",
    "prettier": "^1.5.3",
    "typescript": "^2.4.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "ISC"
}
